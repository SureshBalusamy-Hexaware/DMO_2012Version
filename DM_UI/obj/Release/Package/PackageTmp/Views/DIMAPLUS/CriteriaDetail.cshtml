@using System.Configuration
@{
    ViewBag.Title = "CriteriaDetail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@Scripts.Render("~/Scripts/CriteriaDetail.js")
@Scripts.Render("~/Scripts/ProgressBar.js")
<div class="group">
    <div class="form-group width-per-100" style="width:100%;">
        <table id="grdCriteriaDetail" class="scroll" align='center'></table>
        <div id="pgrCriteriaDetail" class="scroll"></div>
    </div>
    <div class="form-group text-align-center form-group-btn width-per-100">
        <input class="form-button" type="button" value="Save" id="btnSave" />
        <input class="form-button" type="button" value="Back" id="btnBack" onclick="location.href='@Url.Action("Criteria")'" />
    </div>
</div>
<input type="hidden" value='@ViewContext.RouteData.Values["controller"]' id="hdnfldVirtualDir" />
